<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>    
    <add key="ENVQA" value="Server =https://cigniti3.my.salesforce.com/;ReportsPath =C:\Reports;Version=7.5.0;Support=support;" />
    <add key="URL" value="www.google.com" />
    <add key="Environment" value="ENVQA" />
    <!-- Put 'S' for Sequential mode of execution; 'P' for Parallel mode of execution-->
    <add key="ExecutionMode" value="P" />
    <add key="TestsDLLName" value="Automation.Ui.Tests.dll" />
    <add key="ScreenShotForPassedSteps" value="No" />
    <add key="MaxDegreeOfParallelism" value="3" />
    <add key="Application" value="Etrak" />
    <add key="UITestResultPath" value ="C:\SalesForce\Reports\UItestResults.xml"/>
    <!-- Put 'Chrome' for chrome,'Firefox' for firefox, 'IE' for ie browses. 'BrowserStackiOSSafari' for iOS Mobile and BrowserStackAndroidChrome for Android Mobile. 'AndroidChrome' for local android -->
    <add key="DefaultBrowser" value="Chrome" />
    <add key="Chrome" value="target:local; browser:Chrome" />
    <add key="Firefox" value="target:local; browser:Firefox" />
    <add key="IE" value="target:local; browser:IE" />
    <add key="BrowserStackiOSSafari" value="target:local; browser:BrowserStackiOSSafari" />
    <add key="AndroidChrome" value="target:local; browser:AndroidChrome" />
    <add key="BrowserStackAndroidChrome" value="target:local; browser:BrowserStackAndroidChrome" />
    <add key="BS_iPadPro" value="target:local; browser:BS_iPadPro" />
    <add key="BS_MacSafari" value="target:local; browser:BS_MacSafari" />
    <add key="BS_Firefox" value="target:local; browser:BS_Firefox" />
    <add key="BS_IE" value="target:local; browser:BS_IE" />    
    <!-- TestData files : Set value (1) 'UAT' for UAT environment, (2) 'QA' for QA environment-->
    <add key="TestDataFiles" value="RC" />
    <!-- Test Cases : Set value "NO" to load all the test cases, 'YES' for specific test cases from TestcasesToRun.txt file in TestData folder. -->
    <add key="LoadTestCasesFromExternalFile" value="NO" />
    <!-- Put 'YES' or 'NO'-->
    <add key="SendEmail" value="NO" />
    <add key="SendEmailTo" value="Update it" />
    <add key="SendMailFrom" value="Update it" />
    <add key="Password" value="******" />
    <add key="ClientSettingsProvider.ServiceUri" value="a" />
    <add key="BComparerExecutablePath" value="C:\Program Files\Beyond Compare 4\BCompare.exe" />
    <add key="EmailHostName" value="outlook.office365.com" />
    <add key="EmailPort" value="995" />
    <add key="EmailUseSsl" value="true" />
    <add key="EmailId" value="testautomation@maximtrak.com" />
    <add key="Password" value="Bun51497" />    
  </appSettings>

  <startup>
